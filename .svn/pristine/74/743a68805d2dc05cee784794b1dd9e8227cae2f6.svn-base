using TinyMessenger;

namespace OpenCBS.ArchitectureV2.Message
{
    public class EditClientMessage : ITinyMessage
    {
        public EditClientMessage(object sender, int id)
        {
            Sender = sender;
            Id = id;
        }

        public object Sender { get; private set; }

        public int Id { get; private set; }
    }
}
